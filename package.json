{
    "name": "disableligatures",
    "displayName": "Disable Ligatures",
    "description": "Disable ligatures at the cursor position, or disable all ligatures on the line",
    "version": "0.0.10",
    "publisher": "CoenraadS",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "img/icon.png",
    "activationEvents": [
        "*"
    ],
    "keywords": [
        "Ligatures",
        "Ligature"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^10.2.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/CoenraadS/vscode-Disable-Ligatures"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "DisableLigatures",
            "properties": {
                "disableLigatures.mode": {
                    "description": "Mode to disable Ligatures",
                    "type": "string",
                    "enum": [
                        "Cursor",
                        "Line"
                    ],
                    "default": "Cursor"
                },
                "disableLigatures.ligatures": {
                    "description": "Ligatures to disable",
                    "type": "array",
                    "default": [
                        ".=",
                        ".-",
                        ":=",
                        "=:=",
                        "==",
